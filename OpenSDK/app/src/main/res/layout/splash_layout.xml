<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp">

    <EditText
        android:id="@+id/et_appid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="appid"
        android:padding="5dp" />

    <EditText
        android:id="@+id/et_userid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="用户id"
        android:padding="5dp"
        android:text="1545806653070" />

    <CheckBox
        android:id="@+id/cb_env"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="release" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="包名:"
        android:textSize="15dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="com.vhall.live"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="15dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="签名:"
        android:textSize="15dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="F4:E7:21:0F:0F:A1:FC:49:0A:6E:3C:83:33:23:D3:3F:86:2A:3F:05"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="15dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="提示：使用demo演示时，需要使用测试包名和签名，注册测试应用，得到appid"
        android:textSize="12dp" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:onClick="enter"
        android:text="进入" />

</LinearLayout>